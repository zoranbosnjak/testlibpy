            I5 ""
                element 8
                    raw

    061 "Repetitive Regular"
        repetitive 1
            element 8
                raw

    062 "Repetitive With Group"
        repetitive 1
            group
                I1 ""
                    element 8
                        raw
                I2 ""
                    element 8
                        raw

    063 "Repetitive Fx"
        repetitive fx
            element 7
                raw

    071 "Explicit None"
        explicit

    072 "Explicit RE"
        explicit re

    073 "Explicit SP"
        explicit sp

    091 "Compound With One Element"
