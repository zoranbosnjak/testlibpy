            -
            -
            -
            I3 ""
                element 8
                    raw

    101 "Nested Groups"
        group
            SG1 ""
                group
                    EP "Element Populated Bit"
                        element 1
                            raw
                    VAL "Value"
                        element 1
                            raw
            SG2 ""
                group
                    EP "Element Populated Bit"
                        element 1
                            raw
                    VAL "Value"
                        element 1
                            raw
            spare 4

    102 "Nested Groups Extended"
        extended
            I1 ""
                element 7
                    raw
            -
            I2 ""
                group
                    SG1 ""
                        group
                            EP "Element Populated Bit"
                                element 1
                                    raw
                            VAL "Value"
                                element 1
                                    raw
                    SG2 ""
                        group
                            EP "Element Populated Bit"
                                element 1
                                    raw
                            VAL "Value"
                                element 1
                                    raw
                    spare 3
            -

